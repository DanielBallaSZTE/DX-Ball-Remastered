    ver_speed = SET_SPEED;
    hor_speed = -SET_SPEED;
}else if (ran &lt; 0.5 &amp;&amp; ran &gt; 0.25){
    ver_speed = -SET_SPEED;
    hor_speed = SET_SPEED;
}else if (ran &lt; 0.75 &amp;&amp; ran &gt; 0.5) {
    ver_speed = SET_SPEED;
    hor_speed = SET_SPEED;
}else if (ran &lt; 1 &amp;&amp; ran &gt; 0.75) {
    ver_speed = -SET_SPEED;
    hor_speed = -SET_SPEED;
}
