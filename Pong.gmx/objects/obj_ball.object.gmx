        <arguments>
          <argument>
            <kind>1</kind>
            <string>SET_SPEED = 2;
destroy = false;

randomize();
ran = random_range(0, 1)

if (ran &gt; 0 &amp;&amp; ran &lt; 0.25) {
    ver_speed = SET_SPEED;
    hor_speed = -SET_SPEED;
}else if (ran &lt; 0.5 &amp;&amp; ran &gt; 0.25){
    ver_speed = -SET_SPEED;
    hor_speed = SET_SPEED;
}else if (ran &lt; 0.75 &amp;&amp; ran &gt; 0.5) {
    ver_speed = SET_SPEED;
    hor_speed = SET_SPEED;
}else if (ran &lt; 1 &amp;&amp; ran &gt; 0.75) {
    ver_speed = -SET_SPEED;
    hor_speed = -SET_SPEED;
}


tick = 0;
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/*
 * Collisions with objects, bounce offs 
 */

tick++;
// Side of room collisions
if (x - sprite_width / 2 &lt; 0)
{
    hor_speed *= -1;
    x += 1;
}
if (x + sprite_width / 2 &gt; room_width)
{
    hor_speed *= -1;
    x -= 1;
}
if (y - sprite_height / 2 &lt; 0)
{
    ver_speed *= -1;
    y += 1;
}

// Player collisions
if ((distance_to_object(obj_player) &lt; 1) &amp;&amp; (ver_speed &gt; 0) &amp;&amp; y &lt; obj_player.y)
{
    check_pos();
    ver_speed *= -1;
}

// Brick collision system

brick_id = instance_nearest(x, y, obj_brick);
if (instance_exists(brick_id)){
    BRICK_TOP = brick_id.y - brick_id.sprite_height / 2;
    BRICK_BOTTOM = brick_id.y + brick_id.sprite_height / 2;
    BRICK_LEFT = brick_id.x - brick_id.sprite_width / 2;
    BRICK_RIGHT = brick_id.x + brick_id.sprite_width / 2;
    
    coll_x = x;
    coll_y = y;
    
    if (distance_to_object(brick_id) &lt; sqrt(abs(hor_speed * ver_speed)))
    {
        if (x &lt; BRICK_LEFT ) {
            coll_x = x + distance_to_object(brick_id);
        } else if (x &gt; BRICK_RIGHT) {
            coll_x = x - distance_to_object(brick_id);
        }
        if (y &lt; BRICK_TOP) {
            coll_y = y + distance_to_object(brick_id);
        } else if (y &gt; BRICK_BOTTOM) {
            coll_y = y - distance_to_object(brick_id);
        }
    }
    
    top_of_ball = coll_y - sprite_height / 2;
    bottom_of_ball = coll_y + sprite_height / 2;
    right_side_ball = coll_x + sprite_width / 2;
    left_side_ball = coll_x - sprite_width / 2;

    // top of ball, bottom of brick
    if (point_in_triangle(coll_x, top_of_ball, BRICK_LEFT, BRICK_BOTTOM, brick_id.x, brick_id.y, BRICK_RIGHT, BRICK_BOTTOM))
    {
        show_debug_message("bottom " + string(tick));
        ver_speed *= -1;
        destroy = true;
    }
    // bottom of ball, top of brick
    else if (point_in_triangle(coll_x, bottom_of_ball, BRICK_LEFT, BRICK_TOP, brick_id.x, brick_id.y, BRICK_RIGHT, BRICK_TOP))
    {
        show_debug_message("top" + string(tick));
        ver_speed *= -1;
        destroy = true;
    }
    // right side of ball, left side of brick
    else if (point_in_triangle(right_side_ball, coll_y, BRICK_LEFT, BRICK_TOP, brick_id.x, brick_id.y, BRICK_LEFT, BRICK_BOTTOM))
    {
        show_debug_message("left" + string(tick));
        hor_speed *= -1;
        destroy = true;
    }
    // left side of ball, right side of brick
    else if (point_in_triangle(left_side_ball, coll_y, BRICK_RIGHT, BRICK_TOP, brick_id.x, brick_id.y, BRICK_RIGHT, BRICK_BOTTOM))
    {
        show_debug_message("right" + string(tick));
        hor_speed *= -1;
        destroy = true;
    }
    if (destroy)
    {
        with (brick_id)
        {
                instance_destroy();
        }
        destroy = false;
    }
}

if (keyboard_check_released(vk_space))
{
    if (!ver_speed)
    {
        ver_speed = -SET_SPEED;
    }
}

x += hor_speed;
y += ver_speed;
</string>
          </argument>
