          <argument>
            <kind>1</kind>
            <string>/*
 * Endgame screens, restarts etc.
 */
 
if (y &gt; room_height) {
    if (instance_number(obj_ball) == 1)
    {
        ball_out();
    }
    else
    {
        instance_destroy();
    }
}

if (global.p_lives == 0) {
    game_end();    
}

// DEBUG
if (keyboard_check_released (ord ('R')))
{
    power_multiballs();
}
if (keyboard_check_released (ord ('E')))
{
    if (!global.pass_through)
    {
        global.pass_through = true;
    }
    else
    {
        global.pass_through = false;
    }
}
</string>
