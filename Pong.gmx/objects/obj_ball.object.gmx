// Side of room collisions
if (x - sprite_width / 2 &lt; 0)
{
    hor_speed *= -1;
    x += 1;
}
if (x + sprite_width / 2 &gt; room_width)
{
    hor_speed *= -1;
    x -= 1;
}
if (y - sprite_height / 2 &lt; 0)
{
    ver_speed *= -1;
    y += 1;
}

// Player collisions
if ((distance_to_object(obj_player) &lt; 1) &amp;&amp; (ver_speed &gt; 0) &amp;&amp; y &lt; obj_player.y)
{
    check_pos();
    ver_speed *= -1;
}

// Brick collision system

brick_id = instance_nearest(x, y, obj_brick);
// Above, but not outside
if (collision_line(brick_id.x - brick_id.sprite_width / 2, brick_id.y + brick_id.sprite_height / 2, brick_id.x + brick_id.sprite_width / 2, brick_id.y + brick_id.sprite_height / 2, obj_ball, false, false))
{
    if (!global.pass_through)
        ver_speed *= -1;

    with (brick_id)
    {
        if (type == "power")
        {
            generate_power();
        }
        instance_destroy();
    }
}
else if (collision_line(brick_id.x - brick_id.sprite_width / 2, brick_id.y - brick_id.sprite_height / 2, brick_id.x + brick_id.sprite_width / 2, brick_id.y - brick_id.sprite_height / 2, obj_ball, false, false))
{
    if (!global.pass_through)
        ver_speed *= -1;

    with (brick_id)
    {
        if (type == "power")
        {
            generate_power();
        }
        instance_destroy();
    }
}
else if (collision_line(brick_id.x - brick_id.sprite_width / 2, brick_id.y - brick_id.sprite_height / 2, brick_id.x - brick_id.sprite_width / 2, brick_id.y + brick_id.sprite_height / 2, obj_ball, false, false))
{
    if (!global.pass_through)
        hor_speed *= -1;

    with (brick_id)
